node {  
    def mvn-home = tool name: 'mvn-home', type 'maven'
    stage('Git-Checkout') { 
        git branch: 'main', credentialsId: 'git-cred', url: 'https://github.com/Cmisenga/web-app2.git'
    }
    stage('Clean-Complete') { 
        $(mvn-home)/bin/mvn clean compile
    }
    stage('Package') { 
       $(mvn-home)/bin/mvn package      
    }
}
